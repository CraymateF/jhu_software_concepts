# Module 6 environment template
# Copy this file to `.env` and adjust values if needed.

# Flask
FLASK_ENV=production
FLASK_SECRET=your-random-secret-here

# PostgreSQL (db service)
POSTGRES_USER=example_user
POSTGRES_PASSWORD=example_password
POSTGRES_DB=example_db

# App/worker connection strings
DATABASE_URL=postgresql://example_user:example_password@db:5432/example_db
RABBITMQ_URL=amqp://example_user:example_password@rabbitmq:5672/

# Worker seed + ingestion settings
SEED_JSON=/data/applicant_data.json
TARGET_TABLE=gradcafe_main
ID_KEY=url
